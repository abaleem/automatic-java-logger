public class TestCode {

  public static String getString(){
    String str = "Abdullah";
    return str;
  }

  public static int getInt(){
    int num = 1;
    return num;
  }

  public static void main(String[] args){
    int var1 = Integer.parseInt(args[0]) ;
    int var2 = Integer.parseInt(args[1]);
    int var3 = var1 + var2;

    while(var2 < 10){
      var2 = var2+2;
    }

    var3 = getInt();

    if (var3 == 1) {
      String name = "Maaz";
      name = getString();
    }

  }
}